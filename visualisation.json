{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 500,
    "projection": {
      "type": "equalEarth"
    },
    "layer": [
      {
        "data": {
          "url": "https://raw.githubusercontent.com/malim0406/FIT3179/refs/heads/main/ne_110m_admin_0_countries.json",  // TopoJSON URL
          "format": {
            "type": "topojson",
            "feature": "ne_110m_admin_0_countries"
          }
        },
        "mark": {
          "type": "geoshape",
          "fill": "#dddddd",
          "stroke": "white"
        }
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/malim0406/FIT3179/refs/heads/main/obesity-cleaned.csv",  // Your obesity dataset
          "format": {"type": "csv"}
        },
        "mark": "circle",
        "encoding": {
          "longitude": {"field": "Longitude", "type": "quantitative"},
          "latitude": {"field": "Latitude", "type": "quantitative"},
          "size": {"field": "Obesity (%)", "type": "quantitative", "title": "Obesity Rate (%)"},
          "color": {"field": "Country", "type": "nominal"},
          "tooltip": [
            {"field": "Country", "type": "nominal", "title": "Country"},
            {"field": "Obesity (%)", "type": "quantitative", "title": "Obesity Rate (%)"},
            {"field": "Year", "type": "temporal", "title": "Year"}
          ]
        }
      }
    ]
  }
  
  